{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": ["src/routes/**/*.tsx!"],
  "project": ["src/**/*.{ts,tsx}"],
  "ignore": [
    "**/*.test.{ts,tsx}",
    "**/*.spec.{ts,tsx}",
    "playwright/**",
    "e2e/**",
    "test-results/**",
    ".react-router/**",
    "coverage/**",
    "src/lib/compose-refs.ts",
    "src/types/**",
    "src/components/lang-toggle.tsx",
    "src/components/mode-toggle.tsx",
    "src/components/data-table/**",
    "src/components/ui/card.tsx",
    "src/components/ui/faceted.tsx",
    "src/components/ui/sortable.tsx",
    "src/components/ui/checkbox.tsx",
    "src/lib/parsers.ts"
  ],
  "ignoreDependencies": [
    "@types/*",
    "react-dom",
    "tw-animate-css",
    "tailwindcss",
    "@lhci/cli",
    "size-limit",
    "depcheck",
    "@testing-library/dom",
    "prettier",
    "react-router-devtools",
    "stylelint",
    "i18next-cli",
    "@radix-ui/react-visually-hidden",
    "shadcn",
    "ogl",
    "@dnd-kit/core",
    "@dnd-kit/modifiers",
    "@dnd-kit/sortable",
    "@radix-ui/react-dropdown-menu",
    "motion",
    "@react-router/serve",
    "tsx",
    "vite-tsconfig-paths"
  ],
  "ignoreExportsUsedInFile": true,
  "ignoreBinaries": [
    "tsc",
    "stylelint",
    "depcheck",
    "react-router-serve",
    "eslint",
    "prettier",
    "size-limit",
    "playwright",
    "lhci",
    "tsx",
    "i18next-cli",
    "react-router",
    "knip",
    "vitest"
  ]
}
