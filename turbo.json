{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "db:push": {
      "cache": false,
      "persistent": true
    },
    "db:studio": {
      "cache": false,
      "persistent": true
    },
    "db:migrate": {
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false,
      "persistent": true
    },
    "check:types": {
      "dependsOn": ["^check:types"],
      "outputs": [],
      "cache": true
    },
    "check:lint": {
      "dependsOn": ["^check:lint"],
      "outputs": [],
      "cache": true
    },
    "check:knip": {
      "dependsOn": ["^check:knip"],
      "outputs": [],
      "cache": true
    },
    "check:dep": {
      "dependsOn": ["^check:dep"],
      "outputs": [],
      "cache": true
    },
    "check:test": {
      "dependsOn": ["^check:test"],
      "outputs": [],
      "cache": true
    },
    "check:all": {
      "dependsOn": ["check:types", "check:lint", "check:knip", "check:dep"],
      "outputs": [],
      "cache": true
    }
  }
}
